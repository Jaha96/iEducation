{
  "name": "Go",
  "version": "1.0.0",
  "description": "Go CMS",
  "main": "server.js",
  "scripts": {
    "setup-global": "npm install nodemon pm2 -g",
    "setup-dev": "npm install autoprefixer babel-core babel-eslint babel-loader babel-polyfill babel-preset-es2015 babel-preset-react babel-preset-react-hmre babel-preset-stage-2 babel-register chai css-loader eslint eslint-loader extract-text-webpack-plugin file-loader mocha node-sass postcss-loader precss print-message sass-loader strip-loader style-loader url-loader webpack webpack-dev-middleware webpack-dev-server webpack-hot-middleware yargs --save-dev",
    "setup": "npm install immutable react react-dom react-redux react-router redux redux-logger redux-saga --save",
    "setup-go": "npm install bootstrap-sass jquery-placeholder jquery-slimscroll jquery-touchswipe leaflet material-design-icons  simple-line-icons --save",
    "setup-tp": "npm install font-awesome react-bootstrap moment numeral react-draggable react-dropzone-component react-google-maps react-s-alert react-select react-widgets redux-thunk --save",
    "start": "NODE_ENV=development BABEL_ENV=development nodemon ./server.js --hot",
    "start-tp": "NODE_ENV=development BABEL_ENV=development nodemon ./tp/core/server.js --hot",
    "start-node": "NODE_ENV=development BABEL_ENV=development node ./server.js --hot",
    "lint": "eslint -c .eslintrc src",
    "test": "mocha --compilers babel-register --colors -w ./test/*.spec.js",
    "production": "BABEL_ENV=production webpack -p --progress --colors --config webpack/webpack.config.prod.js",
    "watch-sass": "sass --watch resources/assets/go/sass/pages/login.scss:public/css/login.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://n0m4dz@bitbucket.org/mongol/go.git"
  },
  "keywords": [
    "Go",
    "Admin"
  ],
  "author": "n0m4dz",
  "license": "ISC",
  "homepage": "https://bitbucket.org/mongol/go#readme",
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.8.0",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-2": "^6.5.0",
    "babel-register": "^6.8.0",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "eslint": "^2.9.0",
    "eslint-loader": "^1.3.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "mocha": "^2.4.5",
    "node-sass": "^3.7.0",
    "postcss-loader": "^0.9.1",
    "precss": "^1.4.0",
    "print-message": "^2.1.0",
    "sass-loader": "^3.2.0",
    "strip-loader": "^0.1.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.0",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.10.0",
    "yargs": "^4.7.0"
  },
  "dependencies": {
    "bootstrap-sass": "^3.3.6",
    "font-awesome": "^4.6.1",
    "immutable": "^3.8.1",
    "jquery": "^2.2.3",
    "jquery-placeholder": "^2.3.1",
    "jquery-slimscroll": "^1.3.6",
    "jquery-touchswipe": "^1.6.15",
    "leaflet": "^0.7.7",
    "moment": "^2.13.0",
    "numeral": "^1.5.3",
    "perfect-scrollbar": "^0.6.11",
    "react": "^15.0.2",
    "react-bootstrap": "^0.29.3",
    "react-dom": "^15.0.2",
    "react-draggable": "^2.0.0-beta3",
    "react-dropzone-component": "^1.0.2",
    "react-google-maps": "^4.10.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.0",
    "react-s-alert": "^1.1.3",
    "react-select": "^1.0.0-beta13",
    "react-widgets": "^3.3.1",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-saga": "^0.10.2",
    "redux-thunk": "^2.0.1",
    "simple-line-icons": "^2.3.0"
  }
